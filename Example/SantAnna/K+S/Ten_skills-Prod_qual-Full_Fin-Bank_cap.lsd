// disable/enable full logging (FASTMODE=0/1/2)
#define FASTMODE 1								


/*======================== ADDITIONAL CODE TO INCLUDE ========================*/

// C++ additional standard libraries
#include <algorithm>							// sort library
#include <vector>								// vector container library

// LSD and K+S macros and objects definition and support code
#include "fun_head_fast.h"						// LSD definitions
#include "JEDC2010/KS_country.h"				// K+S class definitions
#include "fun_KS_support.h"						// K+S support C++ functions

// define variables to use in equations
#undef EQ_USER_VARS
#define EQ_USER_VARS set::iterator it, it1;		// iterators to list sets


/*============================== LSD EQUATIONS ===============================*/

MODELBEGIN

// The equation files below contains all LSD equations, organized by the
// container object.

#include "fun_KS_worker.h"						// Worker objects equations
#include "fun_KS_firm2.h"						// Firm2 objects equations
#include "fun_KS_firm1.h"						// Firm1 objects equations
#include "fun_KS_bank.h"						// Bank objects equations
#include "fun_KS_labor.h"						// LaborMkt object equations
#include "fun_KS_consumer.h"					// ConsumerMkt object equations
#include "fun_KS_capital.h"						// CapitalMkt object equations
#include "fun_KS_financial.h"					// FinancialMkt object equations
#include "fun_KS_country.h"						// Country object equations
#include "fun_KS_stats.h"						// statistics only equations
#include "JEDC2010/KS_lsd_bridge.h"				// sync LSD variables with K+S

MODELEND


/*=========================== GARBAGE COLLECTION =============================*/

void close_sim( void )
{
	object *cur, *cur1;
	CYCLES( root, cur, "Country" )				// scan all country objects
	{
		CYCLES( cur, cur1, "Firm2" )			// free Firm2 extensions
			DELETE_EXTS( cur1, extFirm2 );
		CYCLES( cur, cur1, "Worker" )			// free Worker extensions
			DELETE_EXTS( cur1, extWorker );
		DELETE_COUNTRY( cur );					// reclaim allocated memory
	}
}

